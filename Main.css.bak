body {
  font-family: Arial, Helvetica, sans-serif;
  font-size: 15px;
}

body{
	background-image: url("homebg.jpeg") ;
	margin:0;
	Height:100%;
	background-position: center;
	background-repeat: no-repeat;
	background-size: cover;
	
}
	
ul{
	list-style-type:none;
	background-color:#ffcdb0;
	margin:0;
	padding:0;
	overflow:hidden;
	position:fixed;
	top:0;
	width:100%
}

li {
	float: left;
}

li a { 
	display:block;
	padding: 15px;
	color:black;
	text-align:center;
	text-decoration:none;
}

li a:hover:not(.active){
	background-color:#f18468;
}	

.active{
	background-color:#ffffda;
}

.logo {
	 display: block;
	 margin-left: auto;
	 margin-right: auto;
	 padding: 10px;
	 width: 15%;
 }
 
.title{
	text-align: center;
}

.pallette{
	float:left;
	width:25%;
	padding:60px;
}

.last{
	clear:left;
}

img.pallette:hover {
  opacity: 0.6;
}

a.texteditor:link { color : black;}
a.texteditor:visited { color : purple;}
a.texteditor:hover { color : red;}
a.texteditor:active { color : yellow;}

